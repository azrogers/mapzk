	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in library 'clauser'",
			"cargo": {
				"args": [
					"test",
					"--target=x86_64-pc-windows-gnu",
					"--no-run",
					"--lib",
					"--package=clauser"
				],
				"filter": {
					"name": "clauser",
					"kind": "lib"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
